DefaultComponentTag=PathPlanner

# Provided Interfaces
Provides.PathPlanner2d.Type=PathPlanner2d
Provides.PathPlanner2d.DefaultName=pathplanner2d

Provides.SkeletonGraphics.Type=QGraphics
Provides.SkeletonGraphics.DefaultName=skeletongraphics

# Required Interfaces
Requires.OgMap.Type=OgMap
Requires.OgMap.DefaultProxy=ogmap@local/ogmaploader

# Config Parameters
Config.Algorithm.Comment=Options are { 'simplenav', 'skeletonnav', 'sparseskeletonnav', 'astar' }
Config.Algorithm.Default=sparseskeletonnav

Config.TraversabilityThreshhold.Default=0.3
Config.RobotDiameterMetres.Default=0.8
Config.DoPathOptimization.Default=1
Config.JiggleWaypointsOntoClearCells.Default=1
Config.IntermediateWaypointMinDistanceTolerance.Default=1.5

Config.Skeleton.Cost.DistanceThreshold.Default=0.3
Config.Skeleton.Cost.CostMultiplier.Default=10.0
Config.Skeleton.SparseSkelAddExtraNodes.Default=1
Config.Skeleton.SparseSkelExtraNodeResolution.Default=5.0
Config.Skeleton.ProvideGraphics.Default=1

# Ice default message size too small for displaying debugging graphics on large maps
ExternalConfig.Ice.MessageSizeMax.Default=3000