# component
DefaultComponentTag=OrcaView3d

# required interfaces
Requires.Laser.Type=LaserScanner2d
Requires.Laser.DefaultProxy=laserscanner2d@local/laser2d

Requires.Position3d.Type=Position3d
Requires.Position3d.DefaultProxy=position3d@local/insgps

# Config Parameters
Config.General.FactoryLibNames.Default=libOrcaQGui3dFactory.so
Config.General.DisplayRefreshTime.Default=200
Config.General.DumpPath.Default = /tmp
Config.ScreenCapture.TopPad.Default = 25
Config.ScreenCapture.SidePad.Default = 2
Config.ScreenCapture.BottomPad.Default = 2
Config.ScreenCapture.CaptureTimerInterval.Default = 1000

# external configuration options
ExternalConfig.Ice.Override.ConnectTimeout.Default=150
